pipeline {
    agent any
    stages {
        stage('PARAMS') {
            steps {
                script {
                  echo 'test'
                  echo "${params.ENV}"
                  echo "${params.VERSION}"
                }
            }
       }
        stage('CLEAN/PRUNE DOCKER IMAGES') {
            steps {
                 sh 'docker image prune --force'
                 sh 'docker image prune -a --force'
            }
       }
  
    }
}
